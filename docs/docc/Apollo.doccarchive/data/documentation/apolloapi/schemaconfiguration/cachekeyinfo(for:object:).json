            },
            {
              "identifier" : "doc:\/\/ApolloAPI\/documentation\/ApolloAPI\/CacheKeyInfo",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " describing the computed cache key for the response object.",
